
    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Name</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
            {{#each model as |workouts|}}
                <tr class="table-striped">
                    <td>
                        {{workouts.name}}
                    </td>
                    <td>
                        {{#link-to 'workouts.edit' workouts tagName="button" }}Edit{{/link-to}}
                    </td>
                    <td>
                        <button {{action 'delete' workouts.id}}>Delete</button>
                    </td>
                </tr>
                {{else}}
                    <tr class="table-striped">
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
            {{/each}}
        </tbody>
    </table>
    {{#link-to 'workouts.create' tagName="button" }}Create{{/link-to}}

